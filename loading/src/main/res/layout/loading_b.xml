<?xml version="1.0" encoding="utf-8"?>
<android.support.percent.PercentRelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/one"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#747474"
        app:layout_heightPercent="25%"
        app:layout_widthPercent="50%">
        <!--章鱼猫-->
        <ProgressBar
            android:id="@+id/pb_loading"
            style="@style/Spinner"
            android:layout_centerInParent="true"
            android:layout_gravity="center|center_horizontal"/>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/three"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/one"
        android:background="#546464"
        app:layout_heightPercent="25%"
        app:layout_widthPercent="50%">


        <ProgressBar
            style="@style/mProgress_circle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:indeterminateDuration="7500"/>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/four"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/one"
        android:background="#000000"
        app:layout_heightPercent="25%"
        app:layout_widthPercent="50%">


        <com.ywzheng.loading.widget.IQiYiLoadingView
            style="@style/mProgress_circle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:indeterminateDuration="1500"
            app:layout_heightPercent="20%"
            app:layout_marginLeftPercent="5%"
            app:layout_marginRightPercent="5%"
            app:layout_widthPercent="30%"/>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/five"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/three"
        android:background="#102030"
        app:layout_heightPercent="25%"
        app:layout_widthPercent="50%">


        <ProgressBar
            style="@android:style/Widget.ProgressBar.Small"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_centerInParent="true"
            android:indeterminateDrawable="@drawable/layer_list_dialog_animated_spinner"
            android:indeterminateDuration="800"/>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/six"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/four"
        android:background="#402030"
        app:layout_heightPercent="25%"
        app:layout_widthPercent="50%">


        <com.ywzheng.loading.widget.LineWithText
            android:id="@+id/linewithtext"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:indeterminateDuration="1500"/>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/seven"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/five"
        android:background="#100030"
        app:layout_heightPercent="25%"
        app:layout_widthPercent="50%">


        <ProgressBar
            style="@android:style/Widget.Holo.ProgressBar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_gravity="center"/>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/eight"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:background="#330201"
        app:layout_heightPercent="25%"
        app:layout_widthPercent="50%">


        <ProgressBar
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_centerInParent="true"
            android:indeterminateDrawable="@drawable/animated_rotate"/>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/two"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:background="#545454"
        app:layout_heightPercent="25%"
        app:layout_widthPercent="50%">

        <ProgressBar
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:indeterminateDrawable="@drawable/rotate_animdraw2"
            android:indeterminateDuration="1500"/>

    </RelativeLayout>

</android.support.percent.PercentRelativeLayout>
